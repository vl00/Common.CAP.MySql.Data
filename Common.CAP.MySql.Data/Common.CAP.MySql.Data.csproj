<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <OutputType>Library</OutputType>
		<TargetFramework>net10.0</TargetFramework>
		<Nullable>enable</Nullable>
		<PackageTags>CAP;MySQL;MySql.Data</PackageTags>
	</PropertyGroup>

  <PropertyGroup>
    <AssemblyName>Common.CAP.MySql.Data</AssemblyName>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <Version>0.1.0</Version>
    <Authors>vl00</Authors>
    <PackageLicenseExpression></PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <Copyright>2025</Copyright>
    <Description></Description>
  </PropertyGroup>

  <ItemGroup>
    <!--<PackageReference Include="DotNetCore.CAP" Version="8.4.1" />-->
		<PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="10.0.0" />
		<PackageReference Include="MySql.Data" Version="[9.4.0,)" />
	</ItemGroup>
  
	<ItemGroup>
		<ProjectReference Include="$(Cap_SourceRoot)\src\DotNetCore.CAP\DotNetCore.CAP.csproj" />
	</ItemGroup>

</Project>
